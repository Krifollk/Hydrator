  "license": "MIT",
  "version": "0.2.0",
  "authors": [
    {
      "name": "Rostyslav Tymoshenko",
      "email": "krifollk@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*"
  },
  "autoload": {
    "psr-4": {
